{
  "name": "@monitel/my-class",
  "version": "0.1.0",
  "description": "auth ",
  "keywords": [
    ""
  ],
  "main": "./bundles/index.umd.js",
  "module": "./esm5/index.js",
  "es2015": "./esm2015/index.js",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/me-component/auth-session-client"
  },
  "author": "Кириченко Сергей <Sergey.Kirichenko@monitel.com>",
  "license": "Apache License",
  "engines": {
    "node": ">=12",
    "npm": ">=6.0",
    "yarn": "^1.0"
  },
  "scripts": {
    "start": "webpack serve --config config/webpack.config.js --port 3335  --inline --progress --content-base app-test/ --mode development",
    "cleanup": "shx rm -rf dist",
    "prebuild": "npm run cleanup && npm run verify",
    "build": "tsc && tsc --target es2018 --outDir dist/esm2015 && rollup -c config/rollup.config.js && rollup -c config/rollup.config.js --environment NODE_ENV:production",
    "postbuild": "node scripts/copy && npm run size",
    "docs": "typedoc -p . --theme minimal --target 'es6' --excludeNotExported --excludePrivate --ignoreCompilerErrors --exclude \"**/src/**/__tests__/*.*\" --out docs src/",
    "test": "jest  -c ./config/jest.config.js",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "test:ci": "npm t -- --ci",
    "validate-js": "tsc -p ./config && tsc -p ./scripts",
    "verify": "npm run validate-js && npm run style && npm run test:ci",
    "commit": "git-cz",
    "style": "npm run lint",
    "style:fix": "npm run lint:fix",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prerelease": "npm run build",
    "release": "standard-version",
    "postrelease": "node scripts/copy && node scripts/build && npm run release:azure && npm run release:pub",
    "release:azure": "git push --no-verify --follow-tags origin master",
    "release:pub": "cd ./dist && pnpm pack && npm publish",
    "release:npm": "npm publish dist",
    "release:preflight": "npm pack ./dist --dry-run",
    "size": "node scripts/file-size ./dist/bundles/index.umd.min.js ./dist/bundles/index.esm.min.js"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "tslib": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run style && npm test -- --bail --onlyChanged",
      "commit-msg": "commitlint --config config/commitlint.config.js -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@typed-f/either": "0.3.8",
    "@typed-f/function": "0.3.8",
    "@typed-f/lens": "0.3.8",
    "@typed-f/maybe": "0.3.8",
    "@types/ramda": "0.27.41",
    "date-fns": "2.22.1",
    "fp-ts": "2.10.5",
    "ramda": "0.27.1",
    "rxjs": "^7.1.0",
    "subsink": "1.0.2"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "12.2.0",
    "@angular/compiler": "12.0.5",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-typescript": "7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "7.14.5",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-replace": "2.4.2",
    "@types/jest": "26.0.23",
    "@types/json5": "0.0.30",
    "@types/mocha": "^8.2.2",
    "@types/node": "15.12.4",
    "@types/prettier": "2.3.0",
    "@types/shelljs": "0.8.8",
    "@types/webpack-config-utils": "2.3.1",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "brotli-size": "4.0.0",
    "commitizen": "4.2.4",
    "cross-var": "1.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-etc": "1.5.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsdoc": "35.4.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-regexp": "0.12.1",
    "eslint-plugin-sonarjs": "0.8.0-125",
    "eslint-plugin-total-functions": "4.7.2",
    "eslint-plugin-unicorn": "33.0.1",
    "gzip-size": "6.0.0",
    "husky": "6.0.0",
    "jest": "27.0.5",
    "jest-watch-typeahead": "0.6.4",
    "jszip": "3.6.0",
    "kleur": "4.1.4",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "pretty-bytes": "5.6.0",
    "rollup": "2.52.2",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "shx": "0.3.3",
    "source-map-loader": "3.0.0",
    "standard-version": "9.3.0",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "tslib": "1.9.3",
    "typedoc": "0.21.0",
    "typescript": "4.3.4",
    "webpack": "5.40.0",
    "webpack-cli": "4.7.2",
    "webpack-config-utils": "2.3.1",
    "webpack-dev-server": "3.11.2"
  }
}
