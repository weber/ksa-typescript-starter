{
  "name": "@monitel/my-class",
  "version": "0.1.0",
  "description": "auth ",
  "keywords": [
    ""
  ],
  "scripts": {
    "start": "webpack serve --config config/webpack.config.js  --progress  --mode development",
    "cleanup": "shx rm -rf dist",
    "prebuild": "npm run cleanup && npm run verify:build",
    "verify:build": "npm run verify",
    "compile": "rollup -c config/rollup.config.js && rollup -c config/rollup.config.js --environment NODE_ENV:production",
    "build": "tsc --project tsconfig.production.json && npm run compile",
    "postbuild": "node scripts/copy && npm run size",
    "compodoc": "compodoc -p tsconfig.json",
    "test": "jest  -c ./config/jest.config.js",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "test:ci": "npm t -- --ci",
    "validate-js": "tsc -p ./config && tsc -p ./scripts",
    "verify": "npm run validate-js && npm run style && npm run test:ci",
    "commit": "git-cz",
    "style": "npm run lint",
    "style:fix": "npm run lint:fix",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prerelease": "npm run build",
    "release": "standard-version",
    "postrelease": "node scripts/copy && node scripts/build && npm run release:azure && npm run release:pub",
    "release:azure": "git push --no-verify --follow-tags origin master",
    "release:pub": "cd ./dist && npm pack && npm publish",
    "release:npm": "npm publish dist",
    "release:preflight": "npm pack ./dist --dry-run",
    "size": "node scripts/file-size ./dist/bundles/index.umd.min.js ./dist/bundles/index.esm.min.js"
  },
  "main": "./bundles/index.umd.js",
  "module": "./esm5/index.js",
  "es2015": "./esm2015/index.js",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Кириченко Сергей <Sergey.Kirichenko@monitel.com>",
  "license": "Apache License",
  "private": true,
  "engines": {
    "node": ">=12",
    "npm": ">=6.0",
    "yarn": ">=1.0"
  },
  "wallaby": {
    "autoDetect": true
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "tslib": "^2.3.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run style && npm test -- --bail --onlyChanged",
      "commit-msg": "commitlint  -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "date-fns": "^2.23.0",
