{
  "name": "@me-component/my-class",
  "version": "0.1.0",
  "description": "auth ",
  "keywords": [
    ""
  ],
  "main": "./bundles/index.umd.js",
  "module": "./esm5/index.js",
  "es2015": "./esm2015/index.js",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/me-component/auth-session-client"
  },
  "author": "Кириченко Сергей <Sergey.Kirichenko@monitel.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "npm": ">=6.0",
    "yarn": "^1.0"
  },
  "scripts": {
    "start": "webpack-dev-server --port 3335  --inline --progress --profile --colors --watch --content-base app-test/ --mode development",
    "cleanup": "shx rm -rf dist",
    "prebuild": "npm run cleanup && npm run verify",
    "build": "tsc && tsc --target es2018 --outDir dist/esm2015 && rollup -c config/rollup.config.js && rollup -c config/rollup.config.js --environment NODE_ENV:production",
    "postbuild": "node scripts/copy && npm run size",
    "docs": "typedoc -p . --theme minimal --target 'es6' --excludeNotExported --excludePrivate --ignoreCompilerErrors --exclude \"**/src/**/__tests__/*.*\" --out docs src/",
    "test": "jest  -c ./config/jest.config.js",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "test:ci": "npm t -- --ci",
    "validate-js": "tsc -p ./config && tsc -p ./scripts",
    "verify": "npm run validate-js && npm run style && npm run test:ci",
    "commit": "git-cz",
    "style": "npm run lint",
    "style:fix": "npm run lint:fix",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "lint:fix": "npm run lint -- --fix",
    "prerelease": "npm run build",
    "release": "standard-version",
    "postrelease": "node scripts/copy && node scripts/build && npm run release:github && npm run release:npm",
    "release:github": "git push --no-verify --follow-tags origin master",
    "release:npm": "npm publish dist",
    "release:preflight": "npm pack ./dist --dry-run",
    "size": "node scripts/file-size ./dist/bundles/index.umd.min.js ./dist/bundles/index.esm.min.js"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --config config/commitlint.config.js -E HUSKY_GIT_PARAMS",
      "pre-commit--": "lint-staged",
      "pre-push": "npm run style && npm test -- --bail --onlyChanged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css,scss,sass,less,md}": [
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "tslib": "^1.11.1"
  },
  "dependencies": {
    "@types/js-cookie": "2.2.6",
    "js-cookie": "^2.2.1",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "source-map-loader": "0.2.4",
    "@babel/plugin-transform-typescript": "7.9.4",
    "awesome-typescript-loader": "5.2.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "7.9.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/jest": "25.2.1",
    "@types/json5": "0.0.30",
    "@types/mocha": "^7.0.2",
    "@types/node": "13.13.2",
    "@types/prettier": "2.0.0",
    "@types/shelljs": "0.8.7",
    "@types/webpack-config-utils": "2.3.0",
    "brotli-size": "4.0.0",
    "commitizen": "4.0.4",
    "cross-var": "1.1.0",
    "cz-conventional-changelog": "3.1.0",
    "gzip-size": "5.1.1",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "jest-watch-typeahead": "0.5.0",
    "jszip": "3.4.0",
    "kleur": "3.0.3",
    "lint-staged": "10.1.7",
    "pretty-bytes": "5.3.0",
    "rollup": "2.7.2",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.5.0",
    "rollup-plugin-terser": "5.3.0",
    "rollup-plugin-uglify": "6.0.4",
    "shx": "0.3.2",
    "standard-version": "7.1.0",
    "ts-jest": "25.4.0",
    "tslib": "1.9.3",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-security": "1.16.0",
    "tslint-config-standard": "9.0.0",
    "tslint-etc": "1.10.1",
    "tslint-loader": "3.6.0",
    "tslint-react": "5.0.0",
    "typedoc": "0.17.4",
    "typescript": "3.8.3",
    "typescript-tslint-plugin": "0.5.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-config-utils": "2.3.1",
    "webpack-dev-server": "3.10.3"
  }
}
